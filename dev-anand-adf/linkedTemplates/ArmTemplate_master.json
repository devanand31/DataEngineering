{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-anand-adf"
		},
		"LS_DATABRICKS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_DATABRICKS'"
		},
		"LS_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().pStorageAccountUrl}"
		},
		"LS_SQL_DB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "@{linkedService().pSqlServerHost}"
		},
		"LS_SQL_DB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().pSqlDbName}"
		},
		"LS_DATABRICKS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0216-053027-7gjpvuy1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-anand-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_DATABRICKS_accessToken": {
						"value": "[parameters('LS_DATABRICKS_accessToken')]"
					},
					"LS_ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_url')]"
					},
					"LS_SQL_DB_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_DB_properties_typeProperties_server')]"
					},
					"LS_SQL_DB_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_DB_properties_typeProperties_database')]"
					},
					"LS_DATABRICKS_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_DATABRICKS_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}